{
   "Statement":[
      {
         "Sid":"PublicReadGetObjectReferer",
         "Effect":"Allow",
         "Principal":"*",
         "Action":"s3:GetObject",
         "Resource":"arn:aws:s3:::BUCKET_NAME/*",
         "Condition":{
            "StringLike":{
               "aws:Referer":[
                  "http://www.example.com/*",
                  "http://example.com/*"
               ]
            }
         }
      }
   ]
}